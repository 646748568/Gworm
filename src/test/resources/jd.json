[
  {
    "id": "Book",
    "gwormArray": {
      "id": "bookList",
      "rule": "#plist .gl-item",
      "list": [
        {
          "id": "bookName",
          "rule": ".p-name",
          "get": "text"
        },
        {
          "id": "bookPage",
          "rule": ".p-name a",
          "get": "href"
        },
        {
          "id": "bookAuthor",
          "rule": ".author_type_1 a",
          "get": "text"
        }
      ]
    }
  }
]

